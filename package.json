{
  "name": "othello",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Nate Geslin <teamtomkins23@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:styles": "cp -R ./src/style/ ./dist/style/",
    "build:scripts": "browserify -d src/script/app.ts -p [ tsify --noImplicitAny ] > dist/bundle.js",
    "build:client": "rm -rf ./dist && mkdir dist && yarn build:scripts && yarn build:styles",
    "test": "mocha -r ts-node/register -r jsdom-global/register src/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "chai-dom": "^1.5.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "sinon": "^4.0.2",
    "ts-node": "^3.3.0",
    "tsify": "^3.0.3"
  },
  "dependencies": {
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  }
}
